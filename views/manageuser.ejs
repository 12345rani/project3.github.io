<font color="white">Welcome admin : <br /><%= sunm %> </font><hr>
       <table class="table table-bordered">
        <tr>
            
            <td>Id</td>
            <td>Name</td>
            <td>EmailId</td>
            <td>Mobile</td>
            <td>Address</td>
            <td>city</td>
            <td>Info</td>
            <td>Gender</td>
            <td>Status</td>
            <td>Action</td>
        </tr>
        <% for(let row of result) {%>
        <tr>
            
            <td><%= row._id %></td>
            <td><%= row.name %></td>
            <td><%= row.email %></td>
            <td><%= row.mobile %></td>
            <td><%= row.address %></td>
            <td><%= row.city %></td>
            <td><%= row.info %></td>
            <td><%= row.gender %></td>
            <td>
                <% if(row.status) { %>
                  <a title="Block User" href="/admin/manageuserstatus?regid=<%= row._id %>&s=block"><img src="../images/block.png" width="30" height="30" ></a>
                 <% } else { %>
                 <a title="Verify User" href="/admin/manageuserstatus?regid=<%= row._id %>&s=verify"><img src="../images/unblock.png" width="30" height="30" ></a>
                  <% } %>
            </td>
            <td><a title="Delete User" href="/admin/manageuserstatus?regid=<%= row._id %>&s=delete"><img src="../images/delete.png" width="30" height="30" ></a></td>
        </tr>    
        <% } %>
       </table>

        <!-- start copyright -->
        <%-include ('copyright.ejs')%>
        <!-- end copyright -->

